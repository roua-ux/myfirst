{% extends 'base.html.twig' %}
{% block nav %}
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">
  <img src="https://getbootstrap.com/docs/5.1/assets/brand/bootstrap-logo.svg" alt="" width="30" height="24">
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
    {% for item in mymenu %}
      <li class="nav-item active">
        <a class="nav-link" href="{{ item.route }}">{{item.intitule}} </a>
      </li>
     {% endfor %} 
      
    </ul>
      <ul class="nav-login pull-right">
       <button type="button" class="btn btn-small btn-login">Se connecter</button>
       <button type="button" class="btn btn-small btn-login">S'inscrire</button>
      </ul>
  </div>
</nav>

 {% endblock %}
